{
	"name": "phpbb/static-pages-bundle",
	"description": "A symfony bundle that provides you to create pages simply by creating a template for them (no controller needed).",
	"keywords": ["phpBB", "static", "pages"],
	"type": "symfony-bundle",
	"homepage": "http://www.phpbb.com",
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "phpBB Group",
			"email": "operations@phpbb.com",
			"homepage": "https://phpbb.com/"
		}
	],
	"require": {
		"symfony/symfony": "2.1.*"
	},
	"minimum-stability": "dev",
	"autoload": {
		"psr-0": { "phpBB\\StaticPagesBundle": "" }
	},
	"target-dir": "phpBB/StaticPagesBundle",
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.0-dev"
		}
	}
}
